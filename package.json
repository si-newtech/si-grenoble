{
  "name": "si-grenoble",
  "version": "0.5.0",
  "license": "MIT",
  "private": false,
  "description": "A Simple Twemoji Selector.",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:docs": "vue-tsc --noEmit && vite --config vite.docs.config.ts build",
    "serve": "vite preview",
    "prepublish": "yarn build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/si-grenoble.umd.js",
  "module": "./dist/si-grenoble.es.js",
  "exports": {
    ".": {
      "import": "./dist/si-grenoble.es.js",
      "require": "./dist/si-grenoble.umd.js"
    }
  },
  "dependencies": {
    "twemoji": "^13.1.0",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@types/twemoji": "^12.1.2",
    "@vitejs/plugin-vue": "^1.9.4",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.3.0"
  }
}
